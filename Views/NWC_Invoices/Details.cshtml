@model GhyomAssignment.Models.NWC_Invoices

@{
    ViewData["Title"] = "Details";
}

<h1>التفاصيل</h1>

<div>
    <h4>الفواتير</h4>
    <hr />
    <dl class="row">

        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NWC_Invoices_No)
        </dd>
        <dt class = "col-sm-2">
           رقم الفاتورة
        </dt>
      
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NWC_Rreal_Estate_Types.NWC_Rreal_Estate_Types_Name)
        </dd>

        <dt class="col-sm-2">
            نوع العقار
        </dt>


        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NWC_Subscription_File.Id)
        </dd>
        <dt class="col-sm-2">
            رقم الاشتراك
        </dt>


        <dd class="col-sm-10" style="background-color:dodgerblue">
            @Html.DisplayFor(model => model.NWC_Subscriber_File.NWC_Subscriber_File_Name)
        </dd>
        <dt class="col-sm-2">
            اسم المشترك
        </dt>


        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NWC_Subscription_File.NWC_Subscription_File_Unit_No)
        </dd>
        <dt class="col-sm-2">
            عدد الوحدات
        </dt>




        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NWC_Invoices_Date)
        </dd>
        <dt class = "col-sm-2">
            تاريخ الفاتورة
        </dt>


        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NWC_Invoices_From)
        </dd>
        <dt class = "col-sm-2">
          من تاريخ
        </dt>


        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NWC_Invoices_To)
        </dd>
        <dt class = "col-sm-2">
            الي تاريخ
        </dt>

        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NWC_Invoices_Previous_Consumption_Amount)
        </dd>
        <dt class = "col-sm-2">
            اخر قراءة للعداد
        </dt>


        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NWC_Invoices_Current_Consumption_Amount)
        </dd>
        <dt class = "col-sm-2">
            القراءة الحالية
        </dt>
       
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NWC_Invoices_Amount_Consumption)
        </dd>
        <dt class = "col-sm-2">
           الاستهلاك
        </dt>


        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NWC_Invoices_Service_Fee)
        </dd>

        <dt class = "col-sm-2">
            رسوم الخدمة
        </dt>


        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NWC_Invoices_Tax_Rate)
        </dd>
        <dt class = "col-sm-2">
           نسبة الضريبة
        </dt>

        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NWC_Invoices_Is_There_Sanitation)
        </dd>
        <dt class = "col-sm-2">
            متوفر الصرف الصحي
        </dt>

        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NWC_Invoices_Wastewater_Consumption_Value)
        </dd>
        <dt class = "col-sm-2">
            استهلاك الصرف الصحي
        </dt>

        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NWC_Invoices_Total_Invoice)
        </dd>
        <dt class = "col-sm-2">
            مجموع الفاتورة
        </dt>

        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NWC_Invoices_Tax_Value)
        </dd>
        <dt class = "col-sm-2">
            قيمة الضريبة
        </dt>
       
        <dd class="col-sm-10" style="background-color:lightgreen">
            @Html.DisplayFor(model => model.NWC_Invoices_Total_Bill)
        </dd>
        <dt class = "col-sm-2">
           مجموع الفاتورة
        </dt>
        
       
  

    </dl>
</div>
<div>
    <br />
    <a asp-action="Index">العودة الي القائمة</a>
    <input type="submit" value="طباعة" class="btn btn-success" /> 
</div>
